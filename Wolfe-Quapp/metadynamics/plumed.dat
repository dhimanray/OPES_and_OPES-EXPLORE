# vim:ft=plumed
UNITS NATURAL

p: POSITION ATOM=1

doubleWell: CUSTOM ARG=p.x,p.y PERIODIC=NO ...
  FUNC=2*(x^4+y^4-2*x^2-4*y^2+2*x*y+0.8*x+0.1*y+9.28)
...
potential: BIASVALUE ARG=doubleWell

 
metad: METAD ...
  ARG=p.x
  PACE=100
  BIASFACTOR=20
  HEIGHT=0.5
  SIGMA=0.05 
  GRID_MIN=-3
  GRID_MAX=3
  GRID_BIN=300
  CALC_RCT 
...


PRINT FMT=%g STRIDE=100 FILE=COLVAR ARG=p.x,p.y,metad.bias,metad.rbias,metad.rct

ENDPLUMED

